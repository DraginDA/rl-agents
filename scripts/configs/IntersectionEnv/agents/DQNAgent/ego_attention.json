{
    "base_config": "scripts/configs/IntersectionEnv/agents/DQNAgent/baseline.json",
    "model": {
        "type": "EgoAttentionNetwork",
        "embedding_layer": {
            "type": "MultiLayerPerceptron",
            "layers": [128, 128],
            "reshape": false,
            "in": 7
        },
        "others_embedding_layer": {
            "type": "MultiLayerPerceptron",
            "layers": [128, 128],
            "reshape": false,
            "in": 7
        },
        "self_attention_layer": null,
        "attention_layer": {
            "type": "EgoAttention",
            "feature_size": 64,
            "heads": 1
        },
        "output_layer": {
            "type": "MultiLayerPerceptron",
            "layers": [128,128],
            "reshape": false
        }
    }
}
